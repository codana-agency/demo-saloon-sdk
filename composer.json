{
    "name": "codana/demo-saloon-demo-saloon-sdk",
    "description": "This is a demo project for Saloon made by Codana for the Toerisme Vlaanderen API",
    "type": "library",
    "homepage": "https://github.com/codana/demo-saloon-demo-saloon-sdk",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Codana\\DemoSaloonSdk\\": "src/",
            "Codana\\DemoSaloonSdk\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Noah Gillard",
            "email": "noah@codana.eu"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "sammyjo20/saloon": "^1.2",
        "spatie/data-transfer-object": "^3.8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "pestphp/pest": "^1.21",
        "spatie/ray": "^1.33"
    },
    "scripts": {
        "test": [
            "./vendor/bin/pest"
        ],
        "fix-code": [
            "./vendor/bin/php-cs-fixer fix"
        ]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
